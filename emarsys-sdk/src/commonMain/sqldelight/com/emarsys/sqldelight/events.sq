CREATE TABLE events (
  id TEXT PRIMARY KEY NOT NULL,
  type TEXT NOT NULL,
  name TEXT NOT NULL,
  attributes TEXT,
  timestamp INTEGER NOT NULL,
  json TEXT NOT NULL
);

CREATE INDEX id ON events(id);

selectAll:
SELECT json FROM events ORDER BY timestamp ASC;

insertEvent:
INSERT OR IGNORE INTO events (id, type, name, attributes, timestamp, json) VALUES (?, ?, ?, ?, ?, ?);

deleteAll:
DELETE FROM events;

deleteById:
DELETE FROM events WHERE id = ?;
